{
  "name": "milwad/laravel-crod",
  "description": "Make easy & fast crud",
  "keywords": [
    "milwad",
    "laravel-crod",
    "easy crud"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "milwad khosravi",
      "email": "milwad.dev@gmail.com",
      "homepage": "https://github.com/milwad-dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.6",
    "laravel/framework": "9.*|10.*"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "Milwad\\LaravelCrod\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Milwad\\LaravelCrod\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Milwad\\LaravelCrod\\LaravelCrodServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
